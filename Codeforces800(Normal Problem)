#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        string a, b = "";
        cin >> a;

        for (char c : a) {
            if (c == 'p') b += 'q';
            else if (c == 'q') b += 'p';
            else b += 'w';
        }

        reverse(b.begin(), b.end());
        cout << b << endl;
    }
}
