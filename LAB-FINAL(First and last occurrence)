#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> nums(n);
    for(int i = 0; i < n; i++) cin >> nums[i];

    int x;
    cin >> x;

    auto first = lower_bound(nums.begin(), nums.end(), x);
    auto last = upper_bound(nums.begin(), nums.end(), x);

    if(first == nums.end() || *first != x)
        cout << -1 << " " << -1;
    else
        cout << first - nums.begin() << " " << (last - nums.begin() - 1);
}
