#include <bits/stdc++.h>
using namespace std;

int main() {
    int t; 
    cin >> t;
    while (t--) {
        int n, x, odd = 0, even = 0;
        cin >> n;
        for (int i = 0; i < n; i++) {
            cin >> x;
            if (x % 2) odd++;
            else even++;
        }
        if (odd > 0 && (even > 0 || odd % 2 == 1)) cout << "YES\n";
        else cout << "NO\n";
    }
}
