#include <iostream>
#include <string>
using namespace std;

int main() {
    string s;
    cin >> s;

    string vowels = "aoyeuiAOYEUI";
    string result = "";

    for (char c : s) {
        if (vowels.find(c) == string::npos) {
            result += '.';
            result += tolower(c);
        }
    }

    cout << result;
    return 0;
}
