#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<int> a(n);
        for (int i = 0; i < n; i++) cin >> a[i];

        bool same = true;
        for (int i = 1; i < n; i++)
            if (a[i] != a[0]) same = false;

        if (same) {
            cout << "NO\n";
        } else {
            cout << "YES\n";
            reverse(a.begin(), a.end());
            for (int x : a) cout << x << " ";
            cout << "\n";
        }
    }
}
