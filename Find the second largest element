#include <iostream>
using namespace std;

int main() {
    int arr[] = {12, 35, 1, 10, 34, 1}; // Example array
    int n = sizeof(arr) / sizeof(arr[0]);

    int first = -1, second = -1; // Initialize first and second largest

    for (int i = 0; i < n; i++) {
        if (arr[i] > first) {
            second = first;
            first = arr[i];
        } else if (arr[i] > second && arr[i] != first) {
            second = arr[i];
        }
    }

    cout << "The second largest element is: " << second << endl;

    return 0;
}
