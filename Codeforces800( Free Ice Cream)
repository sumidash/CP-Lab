#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    long long n, x;
    cin >> n >> x;

    long long distress = 0;

    while (n--) {
        char c;
        long long d;
        cin >> c >> d;

        if (c == '+') {
            x += d;
        } else {
            if (x >= d) x -= d;
            else distress++;
        }
    }

    cout << x << " " << distress;
    return 0;
}
